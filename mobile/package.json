{
    "name": "mobile",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "dev": "yarn build:shared && TS_NODE_FILES=true ts-node --transpile-only --project tsconfig.server.json src/main.ts",
        "build": "yarn clean:shared && ttsc --build tsconfig.server.json && next build",
        "build:shared": "ttsc --build ../shared/tsconfig.json",
        "build:es5": "es-check es5 './.next/static/**/*.js' -v",
        "clean": "rimraf .next build",
        "clean:shared": "rimraf ../shared/build",
        "test": "yarn build:shared && LOG_LEVEL=silent jest --silent",
        "test:debug": "yarn build:shared && NODE_ENV='test' node --inspect-brk=0.0.0.0:9229 ../node_modules/.bin/jest",
        "start": "NODE_ENV=production node build/src/main.js"
    },
    "dependencies": {
        "@babel/plugin-syntax-export-namespace-from": "^7.8.3",
        "@types/react-redux": "^7.1.12",
        "antd": "^4.7.0",
        "dequal": "^2.0.2",
        "detect-browser": "^5.2.0",
        "dotenv": "^8.2.0",
        "google-map-react": "^2.1.9",
        "grommet": "^2.16.2",
        "modern-react-qr-reader": "^1.0.11",
        "next": "^10.1.0",
        "next-i18next": "^7.0.1",
        "next-pwa": "^5.0.6",
        "next-redux-wrapper": "^6.0.2",
        "next-transpile-modules": "^4.1.0",
        "raw-loader": "^4.0.2",
        "react": "^16.14.0",
        "react-dom": "^16.14.0",
        "react-feather": "^2.0.8",
        "react-html-parser": "^2.0.2",
        "react-redux": "^7.2.2",
        "redux": "^4.0.5",
        "redux-devtools-extension": "^2.13.8",
        "redux-persist": "^6.0.0",
        "redux-saga": "^1.1.3",
        "shared": "1.0.0",
        "styled-components": "^5.2.1",
        "terser-webpack-plugin": "4.2.3",
        "title-case": "^3.0.2",
        "ttypescript": "^1.5.12",
        "typed-redux-saga": "^1.3.1",
        "typescript-transform-paths": "^2.0.2"
    },
    "devDependencies": {
        "@babel/plugin-proposal-class-properties": "^7.12.1",
        "@babel/preset-typescript": "^7.12.1",
        "@types/jest": "^26.0.4",
        "@types/react": "^16.9.56",
        "@types/react-dom": "^16.9.9",
        "@types/react-html-parser": "^2.0.1",
        "@types/react-qr-reader": "^2.1.3",
        "@types/styled-components": "^5.1.4",
        "@zeit/next-css": "^1.0.1",
        "@zeit/next-less": "^1.0.1",
        "babel-plugin-import": "^1.13.0",
        "babel-plugin-module-resolver": "^4.0.0",
        "babel-plugin-styled-components": "^1.11.1",
        "less": "^3.12.2",
        "less-vars-to-js": "^1.3.0",
        "network-information-types": "^0.1.0",
        "next-compose-plugins": "^2.2.0",
        "null-loader": "^4.0.1",
        "ts-node": "^9.0.0",
        "typescript": "^4.0.3"
    }
}
